from math import*

def Calculator():
    #Ievadam konst.
    IstGarum = 4
    IstPlat = 1
    PlakGar = 2.352
    PlakPlat = 0.712
    #Apreķiņi 
    #Garāka siena 
    if (IstGarum<IstPlat):
        print("Garāka siena ir",IstPlat)
    else:
        print("Garāka siena ir",IstGarum)
    #Cik gab. iepakojuma
    pieces = round(1.74/(PlakGar*PlakPlat))
    print("Pākā plaksnes gab. =>", pieces)
    #Cik plaksnes vajag?
    vajag = int((IstGarum*IstPlat) / (PlakGar*PlakPlat))
    print("Plāksnes gab. =>", vajag)
    #Cik iepak. vajag
    upakov = int(6 / 1.86)
    print("Vajag", upakov, "pakās gab.")
Calculator()
